
# Supabase Configuration
# Copy this file to .env and fill in your actual values

# Your Supabase project URL
# Found in: Supabase Dashboard > Settings > API
VITE_SUPABASE_URL=https://your-project-id.supabase.co

# Your Supabase anon/public key
# Found in: Supabase Dashboard > Settings > API > Project API keys
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# Optional: Supabase service role key (for server-side operations)
# WARNING: Never expose this in client-side code
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Application Configuration
VITE_APP_NAME=TrueVibe
VITE_APP_VERSION=1.0.0

# OAuth Configuration
# These are configured in Supabase Dashboard > Authentication > Providers
# No additional environment variables needed for Gmail OAuth with Supabase

# Development Configuration
VITE_DEV_MODE=true
VITE_LOG_LEVEL=debug

# Production Configuration (for deployment)
VITE_PRODUCTION_URL=https://your-domain.com
